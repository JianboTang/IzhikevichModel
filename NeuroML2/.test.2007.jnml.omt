# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: LEMS_2007Cells.xml 
engine: jNeuroML
mep: results/.test.2007.mep
experiments:
  LSpop:
    observables:
      spike times:
        file: 
          path: results/iz2007_v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: 0
        tolerance: 0.00